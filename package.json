{
  "name": "openbook-cli",
  "version": "0.1.0",
  "description": "CLI for interacting with OpenBook v2 on Solana.",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuanDeFi/openbook-cli.git"
  },
  "author": {
    "name": "QuanDeFi",
    "email": "q@quandefi.co",
    "url": "https://quandefi.co"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node ./dist/cli.js",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write ./",
    "lint": "eslint ./ --ext ts --quiet",
    "test": "mocha './tests/**/*.ts' --require ts-node/register",
    "validate": "yarn lint && yarn format && yarn test"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@openbook-dex/openbook-v2": "^0.2.10",
    "@solana/web3.js": "^1.77.3",
    "big.js": "^6.2.1",
    "dotenv": "^16.4.7",
    "winston": "^3.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.10.7",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "mocha": "^10.0.0",
    "prettier": "^3.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
